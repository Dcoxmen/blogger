<section id="posts" class="container">
    <h1>All Posts</h1>

    {{#each posts}}
    <div class="post">
        <h3>{{member_name}}</h3>
        <p>{{phone_number}}</p>
        <ul>
            <li>{{level_group}}</li>
            <li><a href="mailto:{{email}}" class="btn btn-reverse">Email</a></li>
        </ul>
    </div>
    {{else}}
    <p>No posts available</p>
    {{/each}}

</section>